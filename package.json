    "awesome-typescript-loader": "^5.0.0",
    "concurrently": "^3.5.1",
    "node-sass": "^4.9.0",
    "react-app-rewired": "^1.5.2",
    "sass-loader": "^7.0.1",
    "sp-rest-proxy": "^2.5.10",
    "typescript": "^2.8.3"
  },
  "proxy": "http://localhost:8081"
}
