{
  "name": "sharepoint-react-webpart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-router": "^4.0.24",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.15.1"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
